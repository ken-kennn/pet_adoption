{
  "expo": {
    "name": "Pettio",
    "slug": "Pettio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pet_adoption.png",
    "scheme": "petadoption",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pet_adoption.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.k3nnyyyy.Pettio"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pet_adoption.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pet_adoption.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "129dad38-5f61-4b37-b5ad-9e2bdfb06958"
      }
    }
  }
}
